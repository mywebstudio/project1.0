Options -Indexes

RewriteEngine on

RewriteBase /

#
RewriteCond %{HTTP_HOST} ^www.korvet.net
RewriteRule (.*) https://korvet.net/$1 [R=301,L]
#
RewriteCond %{HTTP:X-Forwarded-Proto} !https
RewriteRule ^ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301,NE]

RewriteRule ^items/(.+)$ /?q=pages/item&alias=$1 [L]
RewriteRule ^items?(.+)$ /?q=pages/list&$1 [L,QSA]
RewriteRule ^items$ /?q=pages/list [L]

RewriteRule ^pages/(.+){?}(.+)$ /?q=pages/newsItem&alias=$1&section=$2 [L]
RewriteRule ^pages?(.+)$ /?q=pages/newsList&$1 [L,QSA]
RewriteRule ^pages$ /?q=pages/newslist&section=1 [L]

RewriteRule ^contacts$ /?q=pages/newsItem&alias=kontakty [L]
RewriteRule ^about$ /?q=pages/newsItem&alias=about [L]

RewriteRule ^singup$ /?q=processorsFolder/singup [L]
RewriteRule ^message$ /?q=processorsFolder/message [L]
RewriteRule ^submit$ /?q=processorsFolder/submit [L]

RewriteCond $1 !^(index\.php|robots\.txt|sitemap\.xml|sitemap\.php|sitemap\.txt|favicon\.ico|sitemap\.xml|i/.*|tmp/.*|upl/.*|lib/ckeditor4/.*)

RewriteRule ^(.*)$ /index.php?q=$1 [L,QSA]

<IfModule mod_deflate.c>
    AddOutputFilterByType DEFLATE application/xml
    AddOutputFilterByType DEFLATE application/xhtml+xml
    AddOutputFilterByType DEFLATE application/rss+xml
    AddOutputFilterByType DEFLATE application/javascript
    AddOutputFilterByType DEFLATE application/x-javascript
    AddOutputFilterByType DEFLATE text/plain
    AddOutputFilterByType DEFLATE text/html
    AddOutputFilterByType DEFLATE text/xml
    AddOutputFilterByType DEFLATE text/css
    SetOutputFilter DEFLATE
</IfModule>